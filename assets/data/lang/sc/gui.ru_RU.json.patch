          }
        },
        "right-mouse-rebind": {
          "right-mouse": {
            "name": "Переназначение правой кнопки мыши",
            "description": "1: Стандартный/нормальный, 2: Ближний бой, 3: Только рывок, 4: Только защита. \\c[2][QoL]"
          }
        }
      },
      "controls": {
        "keys": {
          "twk": {
