        "assist-scale-enemy-hp": {
          "factor": {
            "name": "Множитель HP врагов",
            "description": "Изменяет HP врагов. \\c[1]Требуется перезапуск! \\c[2][assist]"
          }
        },
        "player-position-marker": {
          "enable": {
            "name": "Показывать маркер положения игрока",
